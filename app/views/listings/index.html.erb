<p id="notice"><%= notice %></p>

<h1>Listing Tests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>City</th>
      <th>Country</th>
      <th>Property</th>
      <th>Bedroom</th>
      <th>Bathroom</th>
      <th>People</th>
      <th>Facilities</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.name %></td>
        <td><%= listing.address %></td>
        <td><%= listing.city %></td>
        <td><%= listing.country %></td>
        <td><%= listing.property %></td>
        <td><%= listing.bedroom %></td>
        <td><%= listing.bathroom %></td>
        <td><%= listing.max_people %></td>
        <td><%= listing.facilities %></td>
        <td><%= listing.price %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>